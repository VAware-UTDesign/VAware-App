{
  "expo": {
    "name": "VAware-App",
    "slug": "VAware-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/VA-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": ["@config-plugins/react-native-ble-plx"],
    "splash": {
      "image": "./assets/images/VAWARE.png",
      "resizeMode": "contain",
      "backgroundColor": "#ABD9FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
